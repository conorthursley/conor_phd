function [dy]=DuffingVal(~,y,~,k1,m1,k2,m2)

%-----------------------------------
x1=y(1);
x2=y(2);
%-----------------------------------
% f=input(1)*(2*pi); 

A=[x2;...
    -((k1/m1)*x1+(k2/m1))*(x1^3);
    
    
% Input excitation force
%sinusodial harmonic
% H=(input(2)*(sin(f*t)));
B=[0 1];


%output result as the equation 
dy=A-B';
% dy=A*y;

end
